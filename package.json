{
  "name": "cmp-sale-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5002 --experimental-https --experimental-https-key ./certificates/adhoc-server.key --experimental-https-cert ./certificates/adhoc-server.crt --experimental-https-ca ./certificates/root_ca.crt",
    "dev:notls": "next dev -p 5000",
    "build": "next build",
    "build:notls": "NODE_TLS_REJECT_UNAUTHORIZED=0 pnpm run build",
    "start": "next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "pretest": "npm run lint",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:codegen": "playwright codegen",
    "report": "playwright show-report",
    "install-deps": "playwright install --with-deps"
  },
  "dependencies": {
    "@serwist/next": "^9.1.1",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "classnames": "^2.5.1",
    "dotenv": "^17.2.1",
    "next": "15.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "^0.0.1",
    "serwist": "^9.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.54.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.6.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fake-indexeddb": "^6.0.1",
    "globals": "^16.3.0",
    "idb-keyval": "^6.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "nock": "^14.0.9",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.14.0"
}
