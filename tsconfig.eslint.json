{
  "extends": "./tsconfig.json",
  "include": [
    "next-env.d.ts",
    "custom.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "tailwind/**/*.ts",
    "**/eslint.config.mjs",
    "__tests__/**/*.ts",
    "__tests__/**/*.tsx",
    "__mocks__/**/*.ts",
    "__mocks__/**/*.tsx"
  ]
}
